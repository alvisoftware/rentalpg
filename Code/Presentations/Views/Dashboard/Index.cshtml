@model IEnumerable<RepositoryLayer.CustomeModel.DashbordModel>

@{
    ViewData["Title"] = "Dashbord";
    Layout = "~/Views/Shared/AdminLTE/_Layout.cshtml";
}

<div class="row">
    <div class="col-lg-3 col-6">
        <!-- small box -->
        <div class="small-box bg-info">
            <div class="inner">
                <h3>@Model.FirstOrDefault().avilable</h3>
                <p>Totoal Poperty</p>
            </div>
            <div class="icon">
                <i class="ion ion-bag"></i>
            </div>
            <a href="#" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
        </div>
    </div>
    <!-- ./col -->
    <!-- ./col -->
    <div class="col-lg-3 col-6">
        <!-- small box -->
        <div class="small-box bg-warning">
            <div class="inner">
                <h3>@Model.FirstOrDefault().rent</h3>

                <p>Rented Property</p>
            </div>
            <div class="icon">
                <i class="ion ion-person-add"></i>
            </div>
            <a href="#" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
        </div>
    </div>
</div>

<table class="table table-condensed table-hover">
    <tbody>
        @foreach (var item in Model.FirstOrDefault().upcomingrent)
        {
            <tr>
                <td>@item.propertytitle</td>
                <td>@item.startDate</td>
                <td>@item.endDate</td>
                <td>@item.rentamount</td>
                <td>@item.status</td>
            </tr>
        }
    </tbody>
</table>

