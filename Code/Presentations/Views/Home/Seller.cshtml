@model IEnumerable<Domain_Layer.Models.Owners>
@{
    ViewData["Title"] = "Seller";
    Layout = "~/Views/Shared/AdminLTE/_Layout.cshtml";
}

<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title">Seller List</h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        <table id="example2" class="table table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th>User Name</th>
                                    <th>Email Id</th>
                                    <th>FullName</th>
                                    <th>Created Date</th>
                                    <th>Created Ip</th>
                                    <th>Created By</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach(var item in Model)
                                {
                                    <tr>
                                        <td>
                                            username
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            EmailAddress
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            FullName
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            Created Date
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            Created Ip
                                        </td>
                                    </tr>

                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>